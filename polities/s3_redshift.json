{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::<S3_BUCKET_NAME>/*",
                "arn:aws:s3:::<S3_BUCKET_NAME>"
            ]
        },
        {
            "Effect": "Allow",
            "Action": "redshift-data:*",
            "Resource": "*"
        }
    ]
}